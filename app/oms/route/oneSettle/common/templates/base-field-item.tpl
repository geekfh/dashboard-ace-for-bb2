<td class="operator-block inline-block">
    <select class="opr-select">
        <option hidden class="op-gt" value="gt">大于</option>
        <option hidden class="op-lt" value="lt">小于</option>
        <option class="op-eq" value="eq" selected>等于</option>
        <option class="op-uq" value="uq">不等于</option>
        <option hidden class="op-in" value="in">在</option>
        <option hidden class="op-not_in" value="not_in">不在</option>
    </select>
</td>
