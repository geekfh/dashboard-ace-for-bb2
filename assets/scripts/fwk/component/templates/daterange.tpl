<div class="date-overlay"></div>
<div class="date-range-picker-wrap">
    <input style="width:0px;" class="hack-focus"/>
    <div class="date-range-form">
        <div class="arrow-back"></div>
        <div class="arrow-font"></div>
        <div class="fast-btns-wrap">
            <ul class="inner no-wrap quick-btns-wrap">
                <li><button class="btn-today">今天</button></li>
                <li><button class="btn-yesterday">昨天</button></li>
                <a class="clearfix"></a>
            </ul>
        </div>
        <div class="date-range">
            <div class="inner">
                <div class="start select-line">
                    从
                    <div class="y-select-wrap select-wrap">
                        <select class="start-y y"></select><span class="text-y">年</span>
                    </div>
                    <div class="m-select-wrap select-wrap">
                        <select class="start-m m"></select><span class="text-m">月</span>
                    </div>

                    <div class="d-select-wrap select-wrap">
                        <select class="start-d d"></select><span class="text-d">日</span>
                    </div>
                </div>
                <div class="end select-line">
                    至
                    <div class="y-select-wrap select-wrap">
                        <select class="end-y y"></select><span class="text-y">年</span>
                    </div>
                    <div class="m-select-wrap select-wrap">
                        <select class="end-m m"></select><span class="text-m">月</span>
                    </div>

                    <div class="d-select-wrap select-wrap">
                        <select class="end-d d"></select><span class="text-d">日</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="help-error-sit unit-inner">
            <label hidden class="error"></label>
        </div>
        <div class="bottom">
            <div class="ok btn pull-left">确定</div>
            <div class="cancel btn pull-left">取消</div>
            <a class="clearfix"></a>
        </div>
    </div>
    
</div>